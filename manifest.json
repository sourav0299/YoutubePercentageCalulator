{
  "manifest_version": 2,
  "name": "YouTube Watch Time Tracker",
  "version": "1.0",
  "description": "Track your progress as you watch YouTube videos.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
